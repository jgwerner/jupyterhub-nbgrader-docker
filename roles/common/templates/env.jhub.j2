# configurable-http-proxy (chp) token 
CONFIGURABLE_HTTP_PROXY={{config_auth_proxy_token}}
CONFIGURABLE_HTTP_PROXY_API_URL=http://{{docker_chp_service_name}}:{{docker_proxy_int_proxy_port}}

# course id
COURSE_ID={{course_id}}

# grader service url
GRADER_SERVICE_URL=http://{{course_id}}:8888

# images used for roles
DOCKER_LEARNER_IMAGE={{docker_custom_notebook_learner_image}}
DOCKER_GRADER_IMAGE={{docker_custom_notebook_grader_image}}
DOCKER_INSTRUCTOR_IMAGE={{docker_custom_notebook_instructor_image}}
DOCKER_STANDARD_IMAGE={{docker_custom_notebook_base_image}}

# docker network
DOCKER_NETWORK_NAME={{docker_net}}

# user home directory within container
DOCKER_NOTEBOOK_DIR={{jnb_dir}}

# nbgrader exchange directory
EXCHANGE_DIR={{nbgrader_exch_dir}}

# group's used to determine role, used to determine user's role
GRADER_GROUP={{grader_group}}
STUDENT_GROUP={{student_group}}

# grader's username, which should coincide with home directory name
GRADER_USERNAME={{grader_username}}

# jupyterhub cryptographic key (mostly for cookies)
JUPYTERHUB_CRYPT_KEY={{jhub_crypt_key}}

# jupyterhub api token (mostly used by services)
JUPYTERHUB_API_TOKEN={{jhub_api_token}}
JUPYTERHUB_API_TOKEN_USER={{grader_service_username}}
JUPYTERHUB_API_URL=http://{{docker_jhub_service_name}}:{{docker_jhub_int_api_port}}

# jupyterhub client id, used to identify the grader service with jupyterhub
JUPYTERHUB_CLIENT_ID={{jhub_client_id}}

# jupyterhub docker service name
JUPYTERHUB_SERVICE_NAME={{docker_jhub_service_name}}

NB_UID={{nfs_home_dir_uid}}
NB_GID={{nfs_home_dir_gid}}

# Local root directory on host, regardless if its mounted with NGFS or not
NFS_ROOT={{nfs_root}}

# organization name used with nfs
ORGANIZATION_NAME={{org_name}}
