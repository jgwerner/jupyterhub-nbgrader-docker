# api token should be equal to value set for jupyterhub
JUPYTERHUB_API_TOKEN={{jhub_api_token}}

# base url including scheme (http/https)
JUPYTERHUB_BASE_URL={{jhub_base_url}}

# service url using internal docker container name:port
JUPYTERHUB_SERVICE_URL={{jhub_service_url}}

# externally managed service name
JUPYTERHUB_SERVICE_NAME={{docker_jhub_service_name}}

# jupyterhub api url using proxy name and the proxy's external port
JUPYTERHUB_API_URL=http://{{docker_chp_service_name}}:{{docker_proxy_ext_proxy_port}}/{{jhub_api_path}}

# jupyterhu client id used with oauth2 setup
JUPYTERHUB_CLIENT_ID={{jhub_client_id}}

# jupyterhub service prefix used in service url path
JUPYTERHUB_SERVICE_PREFIX={{jhub_service_prefix}}

# jupyterhub user for grading service
JUPYTERHUB_USER={{grader_username}}

# grader user/uid/gid to restrict access
NB_USER={{grader_username}}
NB_UID={{grader_instructor_uid}}
NB_GID={{grader_gid}}